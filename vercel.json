{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "vercel-python"
    }
  ],
  "routes": [
    { "src": "/predict", "dest": "api/app.py" },
    { "src": "/api/chat", "dest": "api/app.py" },
    { "src": "/", "dest": "api/app.py" },
    { "src": "/static/(.*)", "dest": "static/$1" }
  ]
}
